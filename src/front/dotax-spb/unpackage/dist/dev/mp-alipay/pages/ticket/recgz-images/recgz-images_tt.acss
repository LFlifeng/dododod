.ri-main {
  background-color: #F4F4F4;
}
.head-section {
  position: absolute;
  height: 80rpx;
  left: 0rpx;
  right: 0rpx;
  padding: 0rpx 32rpx;
  background-color: #FFFFFF;
}
.head-section .head-content {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    height: 80rpx;
}
.head-section .head-content view {
      width: 25%;
      color: #666666;
      font-size: 24rpx;
}
.head-section .head-content view text {
        font-size: 28rpx;
        color: #C91702;
        margin-left: 8rpx;
}
.body-section {
  position: absolute;
  top: 80rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  padding: 0px 0px 120rpx 0rpx;
  background-color: #F4F4F4;
}
.body-section .cont-item {
    margin-top: 20rpx;
    background-color: #FFFFFF;
    padding: 0rpx 30rpx 20rpx 30rpx;
}
.body-section .cont-item .item-head {
      height: 80rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.body-section .cont-item .item-head text {
        margin-top: 24rpx;
        font-size: 30rpx;
        color: #666666;
        font-weight: 600;
        letter-spacing: 2rpx;
}
.body-section .cont-item .item-head .head-y {
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        height: 80rpx;
        -webkit-box-pack: justify;
        -webkit-justify-content: space-between;
                justify-content: space-between;
        -webkit-box-align: center;
        -webkit-align-items: center;
                align-items: center;
}
.body-section .cont-item .item-head .head-y .wz {
          font-size: 20rpx;
          color: #C91702;
          font-weight: 300;
          border: solid 1rpx #C91702;
          border-radius: 5rpx;
          padding: 0rpx 10rpx;
          margin-right: 20rpx;
          height: 38rpx;
          line-height: 38rpx;
}
.body-section .cont-item .item-head .head-y .wz-1 {
          color: #09BA07;
          border-color: #09BA07;
}
.body-section .cont-item .item-head .head-y image {
          width: 36rpx;
          height: 40rpx;
}
.body-section .cont-item .item-body {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: start;
      -webkit-justify-content: flex-start;
              justify-content: flex-start;
      position: relative;
}
.body-section .cont-item .item-body .body-z .zt {
        position: absolute;
        top: 4rpx;
        left: 4rpx;
        height: 30rpx;
        width: 80rpx;
        font-size: 16rpx;
        color: #FFFFFF;
        border-radius: 5rpx;
        opacity: 0.8;
        text-align: center;
        line-height: 30rpx;
}
.body-section .cont-item .item-body .body-z .zt-0 {
        background-color: #E50505;
}
.body-section .cont-item .item-body .body-z .zt-1 {
        background-color: #09BB07;
}
.body-section .cont-item .item-body .body-z .zt-2 {
        background-color: #FF7A08;
}
.body-section .cont-item .item-body .body-z image {
        width: 168rpx;
        height: 168rpx;
        background-color: #EEEEEE;
        border-radius: 5rpx;
}
.body-section .cont-item .item-body .body-y {
        padding: 5rpx 0rpx 5rpx 30rpx;
        min-width: 430rpx;
        min-height: 210rpx;
        margin-left: 40rpx;
}
.body-section .cont-item .item-body .body-y .prop-item {
          height: 50rpx;
          line-height: 50rpx;
}
.body-section .cont-item .item-body .body-y .prop-item .label {
            font-size: 26rpx;
            color: #999;
}
.body-section .cont-item .item-body .body-y .prop-item .value {
            color: #999999;
            margin-left: 20rpx;
}
.foot-section {
  position: absolute;
  height: 119rpx;
  bottom: 0rpx;
  left: 0rpx;
  right: 0rpx;
  background-color: #FFFFFF;
}
.foot-section .foot-content {
    padding: 0rpx 40rpx;
    height: 119rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
}
.foot-section .foot-content button {
      width: 100%;
      height: 80rpx;
      line-height: 80rpx;
      margin: 0rpx;
}

