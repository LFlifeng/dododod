.body {
  background-color: #3D94FF;
  height: 100%;
}
.info {
  padding-top: 80rpx;
  padding-bottom: 32rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.info ._div {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    text-align: center;
}
.info image {
    width: 80rpx;
    height: 80rpx;
}
.info .text {
    color: #fff;
}
.rise_info {
  background-color: #fff;
  margin: 32rpx 40rpx;
  margin-top: 0;
  border-radius: 20rpx;
  text-align: center;
  border-bottom: 1rpx solid;
}
.rise_info image {
    width: 320rpx;
    height: 320rpx;
    margin-top: 60rpx;
    margin-bottom: 40rpx;
}
.rise_info .line {
    height: 1rpx;
    background-color: #d8d8d8;
    position: relative;
}
.rise_info .line .l_text {
      position: absolute;
      top: -20rpx;
      left: 40%;
      height: 40rpx;
      line-height: 40rpx;
      padding: 0 8rpx;
      background-color: #fff;
      color: #999;
      font-size: 28rpx;
}
.rise_info .r_info {
    margin: 40rpx 32rpx;
}
.rise_info .props-item:first-child {
    border: none;
}
.rise_info .props-item {
    height: 60rpx;
    line-height: 60rpx;
    margin: 0rpx 20rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.rise_info .props-item .prop-label {
      text-align: left;
}
.rise_info .props-item .prop-value {
      width: 60%;
      color: #666;
      font-size: 28rpx;
      text-align: right;
      overflow: hidden;
}
.rise_info .props-item .prop-label {
      color: #999;
      font-size: 28rpx;
      width: 40%;
}
.btn-group {
  height: 120rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 36rpx;
}
.btn-group button {
    color: #999;
    font-size: 32rpx;
}

