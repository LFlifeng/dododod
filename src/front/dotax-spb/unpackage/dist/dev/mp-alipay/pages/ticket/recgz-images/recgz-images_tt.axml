<view class="ri-main"><view class="head-section"><view class="head-content"><view>本次识别:<text>{{totalCount}}</text>张</view><view>识别成功:<text>{{successCount-duplicateCount}}</text>张</view><view>识别重复:<text>{{duplicateCount}}</text>张</view><view>识别失败:<text>{{failCount}}</text>张</view></view></view><scroll-view class="body-section" scroll-y="true"><block a:for="{{$root.l1}}" a:for-item="items" a:for-index="gidx" a:key="gidx"><block><view class="cont-area"><block a:for="{{items.l0}}" a:for-item="item" a:for-index="iidx" a:key="iidx"><block><view class="cont-item"><view class="item-head"><text>{{item.m0}}</text><view class="head-y"><image src="../../../static/xp/icon-del.png" data-event-opts="{{[['tap',[['handleDelete',[gidx,iidx]]]]]}}" onTap="__e"></image></view></view><view class="item-body"><view class="body-z"><view class="{{(((('zt')+' '+((item.$orig.recgzStatus=='0')?'zt-0':''))+' '+((item.$orig.recgzStatus=='1')?'zt-1':''))+' '+((item.$orig.recgzStatus=='2'||item.$orig.duplicate)?'zt-2':''))}}">{{''+item.m1}}</view><image src="{{item.$orig.invoiceImg}}" data-src="{{item.$orig.invoiceImg}}" data-event-opts="{{[['tap',[['handlePreviewImage',['$event']]]]]}}" onTap="__e"></image></view><view data-event-opts="{{[['tap',[['handleDetail',[gidx,iidx]]]]]}}" class="body-y" onTap="__e"><block a:if="{{item.m2}}"><view class="prop-item"><text class="label">发票代码：</text><text class="value">{{item.$orig.invoiceCode}}</text></view></block><block a:if="{{item.m3}}"><view class="prop-item"><text class="label">发票号码：</text><text class="value">{{item.$orig.invoiceNum}}</text></view></block><block a:if="{{item.m4}}"><view class="prop-item"><text class="label">{{item.m5+"："}}</text><text class="value">{{item.$orig.rider}}</text></view></block><block a:if="{{item.m6}}"><view class="prop-item"><text class="label">{{item.m7+"："}}</text><text class="value">{{item.$orig.invoiceOpenDate}}</text></view></block><block a:if="{{item.m8}}"><view class="prop-item"><text class="label">出发站：</text><text class="value">{{item.$orig.departure}}</text></view></block><block a:if="{{item.m9}}"><view class="prop-item"><text class="label">到达站：</text><text class="value">{{item.$orig.destination}}</text></view></block><block a:if="{{item.m10}}"><view class="prop-item"><text class="label">出发时间：</text><text class="value">{{item.$orig.setOutDate}}</text></view></block></view></view></view></block></block></view></block></block></scroll-view><view class="foot-section"><view class="foot-content"><button disabled="{{totalCount==0}}" type="primary" data-event-opts="{{[['tap',[['handleSubmit',['$event']]]]]}}" onTap="__e">提交</button></view></view></view>