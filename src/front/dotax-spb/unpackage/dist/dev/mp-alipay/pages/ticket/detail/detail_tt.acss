  padding-top: 60rpx;
}
.image-section image {
    width: 440rpx;
    height: 340rpx;
    background-color: #EEEEEE;
    border-radius: 10rpx;
}
.oprt-section {
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
  right: 0rpx;
  padding: 0rpx;
}
.oprt-section .btn-group {
    background-color: #FFFFFF;
    border-top: 1rpx solid #fff;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding: 30rpx 40rpx;
    height: 100rpx;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
}
.oprt-section .btn-group button {
      width: 48%;
      height: 80rpx;
      line-height: 80rpx;
      border-color: #fff;
}
.oprt-section .btn-group button .uni-button:after {
        border: none;
}
.oprt-section .btn-group button[type="primary"] {
      background-color: #3D94FF;
}
.oprt-section-wt {
  height: 120rpx;
}
.oprt-section-wt .btn-group {
    height: 120rpx;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.right_icon {
  width: 16rpx;
  height: 26rpx;
  margin-left: 20rpx;
}
.uni-collapse-cell--hide.uni-collapse-cell {
  background-color: #f4f4f4;
  border: none;
  text-align: center;
}

