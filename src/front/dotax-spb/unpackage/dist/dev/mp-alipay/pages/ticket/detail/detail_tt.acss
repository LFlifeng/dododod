}
.body-section .check-block .cg-cont .hwmx-cont {
        padding: 0rpx 0rpx;
}
.body-section .check-block .cg-cont .hwmx-cont .subject {
          font-size: 28rpx;
          color: #555555;
          border-bottom: double 4rpx #333333;
}
.body-section .check-block .cg-cont .hwmx-cont .hwmx-item {
          padding: 0rpx 10rpx;
          display: -webkit-box;
          display: -webkit-flex;
          display: flex;
          font-size: 26rpx;
          color: #999999;
          border-bottom: solid 1rpx #777777;
}
.body-section .check-block .cg-cont .hwmx-cont .hwmx-item .mc {
            width: 50%;
}
.body-section .check-block .cg-cont .hwmx-cont .hwmx-item .je {
            width: 25%;
            padding-left: 5rpx;
}
.body-section .check-block .cg-cont .hwmx-cont .hwmx-item .se {
            width: 25%;
            padding-left: 5rpx;
}
.body-section .check-block .sb-cont {
      padding: 0rpx 30rpx 20rpx 30rpx;
}
.body-section .check-block .sb-cont .message {
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-align: end;
        -webkit-align-items: flex-end;
                align-items: flex-end;
        font-size: 28rpx;
        color: #FF3333;
        font-weight: 500;
}
.body-section .check-block .sb-cont .message text {
          font-size: 26rpx;
          color: #C91702;
}
.body-section-wt {
