.head-section {
  display: block;
  position: relative;
  height: 88rpx;
  font-size: 30rpx;
  text-align: center;
  background-color: #FFFFFF;
  color: #666666;
  line-height: 88rpx;
}
.head-section .cy-icon {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: absolute;
    top: 0rpx;
    right: 0rpx;
    bottom: 0rpx;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.head-section .cy-icon text {
      margin-right: 30rpx;
      background-repeat: no-repeat;
      font-size: 20rpx;
      background-size: 25rpx;
      border-radius: 6rpx;
      padding: 0rpx 10rpx 0rpx 40rpx;
      background-position: 10rpx center;
}
.head-section .cy-icon .cy-null,
    .head-section .cy-icon .cy-0,
    .head-section .cy-icon .cy-3 {
      color: #F39C39;
      background-image: url(/static/pj/cy-doing.png);
}
.head-section .cy-icon .cy-1 {
      color: #03A401;
      background-image: url(/static/pj/cy-success.png);
}
.head-section .cy-icon .cy-2 {
      color: #CB0606;
      background-image: url(/static/pj/cy-error.png);
}
.body-section {
  position: absolute;
  bottom: 500rpx;
  top: 108rpx;
  left: 0rpx;
  right: 0rpx;
  padding: 15rpx 0rpx 0rpx 0rpx;
  background-color: #f4f4f4;
}
.body-section .body-content {
    padding: 10rpx 0rpx 0rpx 0rpx;
}
.body-section .props-item:first-child {
    border: none;
}
.body-section .props-item {
    font-size: 34rpx;
    border-top: 1rpx solid #E5E5E5;
    padding: 0rpx 32rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    background-color: #fff;
}
.body-section .props-item .prop-label {
      height: 80rpx;
      line-height: 80rpx;
}
.body-section .props-item .prop-value,
    .body-section .props-item .edit-value {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      height: 90rpx;
      line-height: 90rpx;
      padding: 0;
}
.body-section .props-item picker,
    .body-section .props-item .prop-value,
    .body-section .props-item .edit-value {
      width: 60%;
}
.body-section .props-item .prop-label {
      color: #333333;
      font-size: 34rpx;
      width: 40%;
}
.body-section .props-item .prop-value {
      color: #999999;
      font-size: 34rpx;
}
.body-section .props-item .edit-value {
      color: #999;
}
.body-section .check-block {
    padding: 0rpx 0rpx;
}
.body-section .check-block .cg-cont {
      padding: 0rpx 30rpx 20rpx 30rpx;
}
.body-section .check-block .cg-cont .cont-item .label {
        font-size: 28rpx;
        color: #555555;
}
.body-section .check-block .cg-cont .cont-item .value {
        font-size: 26rpx;
